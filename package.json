{
  "name": "@aureooms/js-collections",
  "description": "Python's collections library for JavaScript",
  "version": "4.0.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-collections/issues"
  },
  "dependencies": {
    "@aureooms/js-collections-chainmap": "^4.0.0",
    "@aureooms/js-collections-counter": "^5.0.0",
    "@aureooms/js-collections-defaultdict": "^4.0.0",
    "@aureooms/js-collections-deque": "^4.0.1",
    "@aureooms/js-collections-dict": "^4.0.0",
    "@aureooms/js-collections-namedtuple": "^4.0.0",
    "@aureooms/js-collections-ordereddict": "^4.0.0",
    "@aureooms/js-collections-set": "^4.0.0",
    "@aureooms/js-error": "^4.0.1"
  },
  "devDependencies": {
    "@aureooms/js-compare": "1.4.8",
    "@aureooms/js-functools": "2.0.3",
    "@aureooms/js-itertools": "5.0.1",
    "@aureooms/js-number": "3.1.0",
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/register": "7.13.8",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "7.4.0",
    "nyc": "15.1.0",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.7"
  },
  "files": [
    "lib"
  ],
  "homepage": "http://aureooms.github.io/js-collections/",
  "keywords": [
    "collection",
    "collections",
    "count",
    "counter",
    "defaultdict",
    "dict",
    "hash",
    "javascript",
    "js",
    "list",
    "map",
    "ordereddict",
    "set"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-collections.git",
    "type": "git"
  },
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "dev": "npm run lint -- --fix && npm run cover -- -- -st --fail-fast",
    "esdoc": "esdoc",
    "lint": "true",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava",
    "travis": "npm run lint && npm run cover"
  },
  "sideEffects": false
}
