{
  "name": "@aureooms/js-collections",
  "description": "Python's collections library for JavaScript",
  "version": "3.0.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-collections/issues"
  },
  "dependencies": {
    "@aureooms/js-collections-chainmap": "^3.0.0",
    "@aureooms/js-collections-counter": "^4.0.0",
    "@aureooms/js-collections-defaultdict": "^3.0.1",
    "@aureooms/js-collections-deque": "^3.1.0",
    "@aureooms/js-collections-dict": "^3.0.0",
    "@aureooms/js-collections-namedtuple": "^3.0.0",
    "@aureooms/js-collections-ordereddict": "^3.0.0",
    "@aureooms/js-collections-set": "^3.0.0",
    "@aureooms/js-error": "^3.0.0"
  },
  "devDependencies": {
    "@aureooms/js-compare": "1.4.5",
    "@aureooms/js-functools": "2.0.3",
    "@aureooms/js-itertools": "3.4.0",
    "@aureooms/js-number": "3.1.0",
    "ava": "3.8.1",
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-latest": "6.24.1",
    "codeclimate-test-reporter": "0.5.1",
    "coveralls": "2.13.3",
    "esdoc": "1.1.0",
    "nyc": "11.9.0",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.2.1"
  },
  "homepage": "http://aureooms.github.io/js-collections/",
  "keywords": [
    "collection",
    "collections",
    "count",
    "counter",
    "defaultdict",
    "dict",
    "hash",
    "javascript",
    "js",
    "list",
    "map",
    "ordereddict",
    "set"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-collections.git",
    "type": "git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "test": "ava",
    "esdoc": "esdoc",
    "prepare": "npm run build"
  }
}
